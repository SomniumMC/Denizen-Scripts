## Steel
Steel_Helmet:
    type: item
    debug: false
    material: iron_helmet
    mechanisms:
     custom_model_data: 10003
     unbreakable: true
     attribute_modifiers:
       armor:
         1:
           operation: add_number
           amount: 3
           slot: head
     #hides: all
     #trim:
     #  material: iron
     #  pattern: custom_armor:heavy_steel_plate
     components_patch: minecraft:equippable=<map[slot=string:head;asset_id=string:custom_armor:heavy_steel_plate]>
    display name: <gold>Steel Helmet
    flags:
      lore: <gold>A basic steel helmet.
      durability1: 320

Steel_Chestplate:
    type: item
    debug: false
    material: iron_chestplate
    mechanisms:
     custom_model_data: 10003
     unbreakable: true
     attribute_modifiers:
       armor:
         1:
           operation: add_number
           amount: 8
           slot: chest
     hides: unbreakable|armor_trim|attributes
     #trim:
     #  material: iron
     #  pattern: custom_armor:heavy_steel_plate
     components_patch: minecraft:equippable=<map[slot=string:chest;asset_id=string:custom_armor:heavy_steel_plate]>
    display name: <gold>Steel Chestplate
    flags:
      lore: <gold>A basic steel Chestplate.
      durability1: 320

Steel_Leggings:
